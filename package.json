{
  "version": "0.2.1",
  "scripts": {
    "start": "PORT=3164 umi dev",
    "build": "umi build",
    "predeploy": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "deploy": "gh-pages -d docs"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@antv/g": "^5.0.10",
    "@antv/g-canvas": "^1.0.9",
    "@antv/g-webgl": "^1.0.13",
    "@types/mousetrap": "^1.6.9",
    "ahooks": "^3.1.9",
    "antd": "^4.18.6",
    "mousetrap": "^1.6.5",
    "react": "17.x",
    "react-color": "^2.19.3",
    "react-dom": "17.x",
    "simplify-js": "^1.2.4",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
